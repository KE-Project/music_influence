@prefix : <http://www.semanticweb.org/KEProject/MusicalInfluences#> .
@prefix I0: <https://w3id.org/italia/onto/l0/> .
@prefix mi: <http://www.semanticweb.org/KEProject/MusicalInfluences#> .
@prefix mm: <https://w3id.org/polifonia/ontology/music-meta/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix core: <https://w3id.org/polifonia/ontology/core/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/KEProject/MusicalInfluences#> .

<http://www.semanticweb.org/KEProject/MusicalInfluences> rdf:type owl:Ontology ;
                                                          rdfs:comment "Ontology for representing influences and inspirations in musical compositions."@en .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gDay
xsd:gDay rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonth
xsd:gMonth rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gMonthDay
xsd:gMonthDay rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYearMonth
xsd:gYearMonth rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#time
xsd:time rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/KEProject/MusicalInfluences#acquaintanceOf
mi:acquaintanceOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf mi:musicArtistRelation ;
                  rdf:type owl:SymmetricProperty ;
                  rdfs:domain mi:MusicArtist ;
                  rdfs:range mi:MusicArtist .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#agentGenre
mi:agentGenre rdf:type owl:ObjectProperty ;
              owl:inverseOf mi:genrePertains ;
              rdfs:domain mi:MusicAgent ;
              rdfs:range mi:Genre ;
              rdfs:comment "A property which associates an agent to the genres it is related to." ;
              rdfs:label "agent genre"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#belongsToGenre
mi:belongsToGenre rdf:type owl:ObjectProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( mi:Performance
                                              mi:PieceOfMusic
                                              mi:Recording
                                            )
                              ] ;
                  rdfs:range mi:Genre ;
                  rdfs:comment "Relation between a recording, a performance or a piece of music and the genre it belongs to." ;
                  rdfs:label "belongs to genre"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#birthCity
mi:birthCity rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf core:hasBirthPlace ;
             rdf:type owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain mi:MusicArtist ;
             rdfs:range mi:City ;
             rdfs:comment "Associates music artists to the city they were born in." ;
             rdfs:label "birth city"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#birthCountry
mi:birthCountry rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf core:hasBirthPlace ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain mi:MusicArtist ;
                rdfs:range mi:Country ;
                rdfs:comment "Associates music artists to the country they were born in." ;
                rdfs:label "birth country"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#cityInCountry
mi:cityInCountry rdf:type owl:ObjectProperty ;
                 owl:inverseOf mi:countryHasCity ;
                 rdf:type owl:FunctionalProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain mi:City ;
                 rdfs:range mi:Country ;
                 rdfs:comment "A relation between cities and the country they are in." ;
                 rdfs:label "city in country"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#closeFriendOf
mi:closeFriendOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf mi:acquaintanceOf ;
                 rdf:type owl:SymmetricProperty ;
                 rdfs:domain mi:MusicArtist ;
                 rdfs:range mi:MusicArtist .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#collaboratedWith
mi:collaboratedWith rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf mi:hasMet ,
                                       mm:hasCollaboratedWith ;
                    rdf:type owl:SymmetricProperty ;
                    rdfs:domain mi:MusicAgent ;
                    rdfs:range mi:MusicAgent ;
                    rdfs:comment "A relation between music agents which represents collaboration in any form" ;
                    rdfs:label "collaborated with"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#containsGenre
mi:containsGenre rdf:type owl:ObjectProperty ;
                 owl:inverseOf mi:genreRepresentedIn ;
                 rdf:type owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain mi:Album ;
                 rdfs:range mi:Genre ;
                 rdfs:comment "Relation between an album and the genre of the recordings it contains." ;
                 rdfs:label "contains genre"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#containsRecording
mi:containsRecording rdf:type owl:ObjectProperty ;
                     owl:inverseOf mi:includedInAlbum ;
                     rdf:type owl:AsymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain mi:Album ;
                     rdfs:range mi:Recording ;
                     rdfs:label "contains recording"@en ,
                                "A relation between albums and the recordings of pieces of music they contain." .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#countryHasCity
mi:countryHasCity rdf:type owl:ObjectProperty ,
                           owl:InverseFunctionalProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain mi:Country ;
                  rdfs:range mi:City ;
                  rdfs:comment "This relation links countries to the cities they contain." ;
                  rdfs:label "country has city"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#coversMusicRole
mi:coversMusicRole rdf:type owl:ObjectProperty ;
                   owl:inverseOf mi:roleCoveredBy ;
                   rdf:type owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain mi:MusicArtist ;
                   rdfs:range mi:MusicRole ;
                   rdfs:comment "This relation associates individual music artists to the music role(s) they cover." ;
                   rdfs:label "covers music role"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#createsAlbum
mi:createsAlbum rdf:type owl:ObjectProperty ;
                owl:inverseOf mi:hasAlbumAuthor ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain mi:MusicAgent ;
                rdfs:range mi:Album ;
                rdfs:comment "A relation that links a music agent (either a group or an individual) to the album(s) whose creation they partecipate in." ;
                rdfs:label "creates album"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#createsLyrics
mi:createsLyrics rdf:type owl:ObjectProperty ;
                 owl:inverseOf mi:hasLyricsAuthor ;
                 rdf:type owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain mi:MusicAgent ;
                 rdfs:range mi:Lyrics ;
                 rdfs:comment "A relation that links a music agent (either a group or an individual) to lyrics of some pieces of music they create." ;
                 rdfs:label "creates lyrics"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#createsRecording
mi:createsRecording rdf:type owl:ObjectProperty ;
                    owl:inverseOf mi:hasRecordingAuthor ;
                    rdf:type owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ;
                    rdfs:domain mi:MusicAgent ;
                    rdfs:range mi:Recording ;
                    rdfs:comment "A relation that links a music agent (either a group or an individual) to recordings of some pieces of music they create." ;
                    rdfs:label "creates recording"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#createsScore
mi:createsScore rdf:type owl:ObjectProperty ;
                owl:inverseOf mi:hasScoreAuthor ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain mi:MusicAgent ;
                rdfs:range mi:Score ;
                rdfs:comment "A relation that links a music agent (either a group or an individual) to scores of some pieces of music they create." ;
                rdfs:label "creates score"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#genrePertains
mi:genrePertains rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain mi:Genre ;
                 rdfs:range mi:MusicAgent ;
                 rdfs:comment "This relations links a given music genre to the music agents that are exponents of the genre." ;
                 rdfs:label "genre pertains"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#genreRepresentedIn
mi:genreRepresentedIn rdf:type owl:ObjectProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain mi:Genre ;
                      rdfs:range mi:Album ;
                      rdfs:comment "A relation which links a given genre to the albums that contain recordings of said genre." .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasAlbumAuthor
mi:hasAlbumAuthor rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf mi:hasAuthor ;
                  rdf:type owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain mi:Album ;
                  rdfs:range mi:MusicAgent ;
                  rdfs:comment "A specialization of the hasAuthor relation which pertains to albums only." ;
                  rdfs:label "has album author"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasAuthor
mi:hasAuthor rdf:type owl:ObjectProperty ,
                      owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( mi:Album
                                         mi:Lyrics
                                         mi:Recording
                                         mi:Score
                                       )
                         ] ;
             rdfs:range mi:MusicAgent ;
             rdfs:comment "A relatively general relation which links albums, lyrics, recordings and scores to the music agent(s) that are their author(s)." ;
             rdfs:label "has author"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasLyricsAuthor
mi:hasLyricsAuthor rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf mi:hasAuthor ;
                   rdf:type owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain mi:Lyrics ;
                   rdfs:range mi:MusicAgent ;
                   rdfs:comment "A specialization of the hasAuthor relation which pertains to lyrics only." ;
                   rdfs:label "has lyrics author"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasMember
mi:hasMember rdf:type owl:ObjectProperty ;
             owl:inverseOf mi:memberOf ;
             rdf:type owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain mi:MusicGroup ;
             rdfs:range mi:MusicArtist ;
             rdfs:comment "A relation which links music groups to their members" ;
             rdfs:label "has member"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasMet
mi:hasMet rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf mi:musicAgentRelation ;
          rdf:type owl:SymmetricProperty ;
          rdfs:domain mi:MusicAgent ;
          rdfs:range mi:MusicAgent ;
          rdfs:comment "A relatively weak form of interaction between music agents which links music agents if they have met at least once" ;
          rdfs:label "has met"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasMusicRole
mi:hasMusicRole rdf:type owl:ObjectProperty ;
                owl:inverseOf mi:roleIncluded ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain mi:MusicGroup ;
                rdfs:range mi:MusicRole ;
                rdfs:comment "A relation which links a music group to the music roles covered by its members." ;
                rdfs:label "has music role"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasRecordingAuthor
mi:hasRecordingAuthor rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf mi:hasAuthor ;
                      rdf:type owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain mi:Recording ;
                      rdfs:range mi:MusicAgent ;
                      rdfs:comment "A specialization of the hasAuthor relation which pertains to recordings only." ;
                      rdfs:label "has recording author"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasScoreAuthor
mi:hasScoreAuthor rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf mi:hasAuthor ;
                  rdf:type owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain mi:Score ;
                  rdfs:range mi:MusicAgent ;
                  rdfs:comment "A specialization of the hasAuthor relation which pertains to scores only." ;
                  rdfs:label "has score author"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasTimeInterval
mi:hasTimeInterval rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf core:isSettingFor ;
                   rdf:type owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain mi:Situation ;
                   rdfs:range mi:TimeInterval ;
                   rdfs:comment "A relation which links a situation to its time interval." ;
                   rdfs:label "has time interval"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#includedInAlbum
mi:includedInAlbum rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain mi:Recording ;
                   rdfs:range mi:Album ;
                   rdfs:comment "The relation which model the membership of a recording within an album." ;
                   rdfs:label "included in album"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#influenced
mi:influenced rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf mi:musicAgentRelation ;
              owl:inverseOf mi:wasInfluencedBy ;
              rdfs:domain mi:MusicAgent ;
              rdfs:range mi:MusicAgent ;
              rdfs:comment "A relation that associates any MusicAgent to any other MusicAgent which the former influenced." ;
              rdfs:label "influenced"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#involvesArtist
mi:involvesArtist rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf core:isSettingFor ;
                  rdf:type owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain mi:Situation ;
                  rdfs:range mi:MusicArtist ;
                  rdfs:comment "A relation that model the involvement of a music artist within a given situation." ;
                  rdfs:label "involves artist"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#involvesCity
mi:involvesCity rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf core:isSettingFor ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain mi:Situation ;
                rdfs:range mi:City ;
                rdfs:comment "A relation that model the involvement of a city within a given situation." ;
                rdfs:label "involves city"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#involvesCountry
mi:involvesCountry rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf core:isSettingFor ;
                   rdf:type owl:AsymmetricProperty ,
                            owl:IrreflexiveProperty ;
                   rdfs:domain mi:Situation ;
                   rdfs:range mi:Country ;
                   rdfs:comment "A relation that model the involvement of a country within a given situation." ;
                   rdfs:label "involves country"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#involvesGroup
mi:involvesGroup rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf core:isSettingFor ;
                 rdf:type owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain mi:Situation ;
                 rdfs:range mi:MusicGroup ;
                 rdfs:comment "A relation that model the involvement of a music group within a given situation." ;
                 rdfs:label "involves group"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#involvesPerformance
mi:involvesPerformance rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf core:isSettingFor ;
                       rdf:type owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:domain mi:Situation ;
                       rdfs:range mi:Performance ;
                       rdfs:comment "A relation that model the involvement of a performance within a given situation." ;
                       rdfs:label "involves performance"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#involvesRole
mi:involvesRole rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf core:isSettingFor ;
                rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain mi:Situation ;
                rdfs:range mi:MusicRole ;
                rdfs:comment "A relation that model the involvement of a music role within a given situation." ;
                rdfs:label "involves role"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#isKnownBy
mi:isKnownBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf mi:musicAgentRelation ;
             owl:inverseOf mi:knowsOf ;
             rdf:type owl:IrreflexiveProperty ;
             rdfs:domain mi:MusicAgent ;
             rdfs:range mi:MusicAgent ;
             rdfs:comment "Associates music agents to other music agents that know the formers." ;
             rdfs:label "is known by"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#isRealisedBy
mi:isRealisedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf core:isRealizedBy ;
                owl:inverseOf mi:realises ;
                rdf:type owl:InverseFunctionalProperty ,
                         owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain mi:PieceOfMusic ;
                rdfs:range mi:PieceOfMusicRealisation ;
                rdfs:comment "A relation which models the Information Realisation pattern: it links a piece of music to its own realisation(s)" ;
                rdfs:label "is realised by"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#knowsOf
mi:knowsOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf mi:musicAgentRelation ;
           rdf:type owl:IrreflexiveProperty ;
           rdfs:domain mi:MusicAgent ;
           rdfs:range mi:MusicAgent ;
           rdfs:comment "Associates music agents to other music agents they know." ;
           rdfs:label "knows of"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#livedInCity
mi:livedInCity rdf:type owl:ObjectProperty ,
                        owl:AsymmetricProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain mi:MusicArtist ;
               rdfs:range mi:City .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#livedInCountry
mi:livedInCountry rdf:type owl:ObjectProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain mi:MusicArtist ;
                  rdfs:range mi:Country ;
                  rdfs:comment "Associates a music artist to the countries he lived in." ;
                  rdfs:label "lived in country"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#memberOf
mi:memberOf rdf:type owl:ObjectProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain mi:MusicArtist ;
            rdfs:range mi:MusicGroup ;
            rdfs:comment "Inverse property of \"hasMember\": this links music artists to the music group(s) they belong to." ;
            rdfs:label "member of"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#mentorOf
mi:mentorOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf mi:musicArtistRelation ;
            rdfs:domain mi:MusicArtist ;
            rdfs:range mi:MusicArtist ;
            rdfs:comment "A property which associates an artist to another artist who was mentored by the former" ;
            rdfs:label "mentor of"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#musicAgentRelation
mi:musicAgentRelation rdf:type owl:ObjectProperty ;
                      rdfs:domain mi:MusicAgent ;
                      rdfs:range mi:MusicAgent ;
                      rdfs:comment "A generic superclass for more specified relations between music agents" ;
                      rdfs:label "music agent relation"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#musicArtistRelation
mi:musicArtistRelation rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf mi:hasMet ;
                       rdf:type owl:SymmetricProperty ;
                       rdfs:domain mi:MusicArtist ;
                       rdfs:range mi:MusicArtist .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#performedWith
mi:performedWith rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf mi:collaboratedWith ;
                 rdf:type owl:SymmetricProperty ;
                 rdfs:domain mi:MusicAgent ;
                 rdfs:range mi:MusicAgent ;
                 owl:propertyChainAxiom ( [ owl:inverseOf mi:involvesArtist
                                          ]
                                          mi:involvesPerformance
                                          [ owl:inverseOf mi:involvesPerformance
                                          ]
                                          mi:involvesArtist
                                        ) ,
                                        ( [ owl:inverseOf mi:involvesArtist
                                          ]
                                          mi:involvesPerformance
                                          [ owl:inverseOf mi:involvesPerformance
                                          ]
                                          mi:involvesGroup
                                        ) ,
                                        ( [ owl:inverseOf mi:involvesGroup
                                          ]
                                          mi:involvesPerformance
                                          [ owl:inverseOf mi:involvesPerformance
                                          ]
                                          mi:involvesArtist
                                        ) ,
                                        ( [ owl:inverseOf mi:involvesGroup
                                          ]
                                          mi:involvesPerformance
                                          [ owl:inverseOf mi:involvesPerformance
                                          ]
                                          mi:involvesGroup
                                        ) ;
                 rdfs:comment "A specialized form of collaboration between music agents which consists in performing together." ;
                 rdfs:label "performed with"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#playedInSamePeriod
mi:playedInSamePeriod rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf mi:musicAgentRelation ;
                      rdf:type owl:SymmetricProperty ;
                      rdfs:domain mi:MusicAgent ;
                      rdfs:range mi:MusicAgent ;
                      rdfs:comment "A relation between music agents which played in the same period of time." ;
                      rdfs:label "playerd in same period"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#playedSameGenre
mi:playedSameGenre rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf mi:musicAgentRelation ;
                   rdf:type owl:SymmetricProperty ;
                   rdfs:domain mi:MusicAgent ;
                   rdfs:range mi:MusicAgent ;
                   owl:propertyChainAxiom ( [ owl:inverseOf mi:genrePertains
                                            ]
                                            mi:genrePertains
                                          ) ;
                   rdfs:comment "A relation between music agents which played music belonging to the same genre" ;
                   rdfs:label "played same genre"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#playsSameGenre
mi:playsSameGenre rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf mi:musicAgentRelation ;
                  rdfs:domain mi:MusicAgent ;
                  rdfs:range mi:MusicAgent ;
                  owl:propertyChainAxiom ( mi:agentGenre
                                           [ owl:inverseOf mi:agentGenre
                                           ]
                                         ) ;
                  rdfs:comment "A property which associates music agents which plays the same genre" ;
                  rdfs:label "plays same genre"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#realises
mi:realises rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf core:realizes ;
            rdf:type owl:FunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain mi:PieceOfMusicRealisation ;
            rdfs:range mi:PieceOfMusic ;
            rdfs:comment "Inverse relation of isRealizedBy, which contributes in modeling the Information Realization pattern: this associates a given piece of music realisation to the piece of music itself." ;
            rdfs:label "realises"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#releaseOfAlbum
mi:releaseOfAlbum rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf core:isSettingFor ;
                  rdf:type owl:FunctionalProperty ,
                           owl:InverseFunctionalProperty ,
                           owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain mi:ReleaseSituation ;
                  rdfs:range mi:Album ;
                  rdfs:comment "Associates a release situation to the released album itself." ;
                  rdfs:label "release of album"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#releaseOfRecording
mi:releaseOfRecording rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf core:isSettingFor ;
                      rdf:type owl:InverseFunctionalProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain mi:ReleaseSituation ;
                      rdfs:range mi:Recording ;
                      rdfs:comment "Associates a release situation to the released recording itself." ;
                      rdfs:label "release of recording"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#roleCoveredBy
mi:roleCoveredBy rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain mi:MusicRole ;
                 rdfs:range mi:MusicArtist ;
                 rdfs:comment "Associates a given music role to the artists that cover that role" ;
                 rdfs:label "role covered by"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#roleIncluded
mi:roleIncluded rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain mi:MusicRole ;
                rdfs:range mi:MusicGroup ;
                rdfs:comment "A relation which links a music role to the music groups which include it." ;
                rdfs:label "role included"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#sharedMusicGroup
mi:sharedMusicGroup rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf mi:collaboratedWith ;
                    rdf:type owl:SymmetricProperty ;
                    rdfs:domain mi:MusicArtist ;
                    rdfs:range mi:MusicArtist ;
                    owl:propertyChainAxiom ( mi:memberOf
                                             mi:hasMember
                                           ) ;
                    rdfs:comment "A specialized form of collaboration between music agents which consists in having a music group in common." ;
                    rdfs:label "shared music group"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#wasInfluencedBy
mi:wasInfluencedBy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf mi:musicAgentRelation ;
                   rdfs:domain mi:MusicAgent ;
                   rdfs:range mi:MusicAgent ;
                   rdfs:comment "Inverse property of influenced" ;
                   rdfs:label "was influenced by"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#wroteLyricsWith
mi:wroteLyricsWith rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf mi:collaboratedWith ;
                   rdf:type owl:SymmetricProperty ;
                   rdfs:domain mi:MusicArtist ;
                   rdfs:range mi:MusicArtist ;
                   owl:propertyChainAxiom ( mi:createsLyrics
                                            mi:hasLyricsAuthor
                                          ) ;
                   rdfs:comment "A specialized form of collaboration between music agents which consists in collaborating in creating the lyrics of a piece of music." ;
                   rdfs:label "wrote lyrics with"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#wroteScoreWith
mi:wroteScoreWith rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf mi:collaboratedWith ;
                  rdf:type owl:SymmetricProperty ;
                  rdfs:domain mi:MusicArtist ;
                  rdfs:range mi:MusicArtist ;
                  owl:propertyChainAxiom ( mi:createsScore
                                           mi:hasScoreAuthor
                                         ) ;
                  rdfs:comment "A specialized form of collaboration between music agents which consists in collaborating in creating the score of a piece of music." ;
                  rdfs:label "wrote score with"@en .


###  https://w3id.org/polifonia/ontology/core/hasBirthPlace
core:hasBirthPlace rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf core:hasPlace ;
                   owl:inverseOf core:isBirthPlaceOf ;
                   rdfs:domain core:Person ;
                   rdfs:range core:Place ;
                   rdfs:label "has birth place"@en .


###  https://w3id.org/polifonia/ontology/core/hasPlace
core:hasPlace rdf:type owl:ObjectProperty ;
              owl:inverseOf core:isPlaceOf ;
              rdfs:domain owl:Thing ;
              rdfs:range core:Place ;
              rdfs:label "has place"@en .


###  https://w3id.org/polifonia/ontology/core/hasSetting
core:hasSetting rdf:type owl:ObjectProperty ;
                owl:inverseOf core:isSettingFor ;
                rdfs:domain owl:Thing ;
                rdfs:range core:Situation ;
                rdfs:comment "A relation between entities and situations, e.g. 'this morning I've prepared my coffee with a new fantastic Arabica' (i.e.: (an amount of) a new fantastic Arabica hasSetting the preparation of my coffee this morning)." ;
                rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/situation.owl> ;
                rdfs:label "has setting"@en .


###  https://w3id.org/polifonia/ontology/core/hasTimeInterval
core:hasTimeInterval rdf:type owl:ObjectProperty ;
                     rdfs:range core:TimeInterval .


###  https://w3id.org/polifonia/ontology/core/isBirthPlaceOf
core:isBirthPlaceOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf core:isPlaceOf ;
                    rdfs:domain core:Place ;
                    rdfs:range core:Person ;
                    rdfs:label "is birth place of"@en .


###  https://w3id.org/polifonia/ontology/core/isPlaceOf
core:isPlaceOf rdf:type owl:ObjectProperty ;
               rdfs:domain core:Place ;
               rdfs:range owl:Thing ;
               rdfs:label "is place of"@en .


###  https://w3id.org/polifonia/ontology/core/isRealizedBy
core:isRealizedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf core:realizes ;
                  rdfs:domain core:InformationObject ;
                  rdfs:range core:InformationRealization ;
                  rdfs:comment "A relation between an information object and its realization (an information realization)."@en ;
                  rdfs:label "is realized by"@en .


###  https://w3id.org/polifonia/ontology/core/isSettingFor
core:isSettingFor rdf:type owl:ObjectProperty ;
                  rdfs:domain core:Situation ;
                  rdfs:range owl:Thing ;
                  rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/situation.owl> ;
                  rdfs:label "is setting for"@en .


###  https://w3id.org/polifonia/ontology/core/isTimeIntervalOf
core:isTimeIntervalOf rdf:type owl:ObjectProperty ;
                      rdfs:domain core:TimeInterval .


###  https://w3id.org/polifonia/ontology/core/realizes
core:realizes rdf:type owl:ObjectProperty ;
              rdfs:domain core:InformationRealization ;
              rdfs:range core:InformationObject ;
              rdfs:comment "A relation between an information realization and an information object."@en ;
              rdfs:label "realizes"@en .


###  https://w3id.org/polifonia/ontology/music-meta/hasCollaboratedWith
mm:hasCollaboratedWith rdf:type owl:ObjectProperty ;
                       rdfs:domain mm:MusicArtist ;
                       rdfs:range mm:MusicArtist ;
                       rdfs:label "Has collaborated with"@en .


###  https://w3id.org/polifonia/ontology/music-meta/isRealisedBy
mm:isRealisedBy rdf:type owl:ObjectProperty ;
                rdfs:range mm:MusicalPerformance .


###  https://w3id.org/polifonia/ontology/music-meta/isRecordedBy
mm:isRecordedBy rdf:type owl:ObjectProperty ;
                rdfs:domain mm:MusicalPerformance .


###  https://w3id.org/polifonia/ontology/music-meta/wasFormedIn
mm:wasFormedIn rdf:type owl:ObjectProperty ;
               rdfs:domain mm:MusicEnsenble ;
               rdfs:range core:Place ;
               rdfs:label "Was formed in"@en .


