               rdfs:label "activity end"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#activityStart
mi:activityStart rdf:type owl:DatatypeProperty ;
                 rdfs:domain mi:MusicAgent ;
                 rdfs:range xsd:date ;
                 rdfs:comment "Relates a music agent to a date at which its activity started." ;
                 rdfs:label "activity start"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#albumTitle
mi:albumTitle rdf:type owl:DatatypeProperty ;
              rdfs:domain mi:Album ;
              rdfs:range xsd:string ;
              rdfs:comment "Data property which associates an album to its title" ;
              rdfs:label "album title"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#birthDate
mi:birthDate rdf:type owl:DatatypeProperty ;
             rdfs:domain mi:MusicArtist ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:unionOf ( xsd:date
                                        xsd:dateTime
                                        xsd:gYear
                                        xsd:gYearMonth
                                      )
                        ] ;
             rdfs:comment "Associates music artists to their birth date." ;
             rdfs:label "birth date"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#deathDate
mi:deathDate rdf:type owl:DatatypeProperty ;
             rdfs:domain mi:MusicArtist ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:unionOf ( xsd:date
                                        xsd:dateTime
                                        xsd:gYear
                                        xsd:gYearMonth
                                      )
                        ] ;
             rdfs:comment "Associates music artists to their death date." ;
             rdfs:label "death date"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#endTime
mi:endTime rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf mi:hasTime ;
           rdfs:domain mi:TimeInterval ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:unionOf ( xsd:date
                                      xsd:dateTime
                                      xsd:gDay
                                      xsd:gMonth
                                      xsd:gMonthDay
                                      xsd:gYear
                                      xsd:gYearMonth
                                      xsd:time
                                    )
                      ] ;
           rdfs:comment "Data property which associates a time interval to its ending time." ;
           rdfs:label "end time"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#hasTime
mi:hasTime rdf:type owl:DatatypeProperty ;
           rdfs:domain mi:TimeInterval ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:unionOf ( xsd:date
                                      xsd:dateTime
                                      xsd:gDay
                                      xsd:gMonth
                                      xsd:gMonthDay
                                      xsd:gYear
                                      xsd:gYearMonth
                                      xsd:time
                                    )
                      ] ;
           rdfs:comment "A datatype property that encodes values for a TimeInterval; a same TimeInterval can have more than one value: start time, end time, times at which the interval holds, as well as times expressed in different formats: xsd:gYear, xsd:dateTime, etc." ;
           rdfs:label "has time"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#musicArtistName
mi:musicArtistName rdf:type owl:DatatypeProperty ;
                   rdfs:domain mi:MusicArtist ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Data property which associates an artist to its name" ;
                   rdfs:label "music artist name"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#musicArtistPseudonym
mi:musicArtistPseudonym rdf:type owl:DatatypeProperty ;
                        rdfs:domain mi:MusicArtist ;
                        rdfs:range xsd:string ;
                        rdfs:comment "Data property which associates an artist to its pseudonym(s)." ;
                        rdfs:label "music artist pseudonym"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#musicGroupName
mi:musicGroupName rdf:type owl:DatatypeProperty ;
                  rdfs:domain mi:MusicGroup ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Data property which associates a music group to its name" ;
                  rdfs:label "music group name"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#onDate
mi:onDate rdf:type owl:DatatypeProperty ;
          rdfs:domain mi:Situation ;
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:unionOf ( xsd:date
                                     xsd:dateTime
                                     xsd:gYear
                                     xsd:gYearMonth
                                   )
                     ] ;
          rdfs:comment "A data property which associates any situation to the date (date, dateTime, gYear or gYearMonth) at which the situation happened, if any." ;
          rdfs:label "on date"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#pieceOfMusicTitle
mi:pieceOfMusicTitle rdf:type owl:DatatypeProperty ;
                     rdfs:domain mi:PieceOfMusic ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Data property which associates a piece of music to its title (if any)." ;
                     rdfs:label "piece of music title"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#startTime
mi:startTime rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf mi:hasTime ;
             rdfs:domain mi:TimeInterval ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:unionOf ( xsd:date
                                        xsd:dateTime
                                        xsd:gDay
                                        xsd:gMonth
                                        xsd:gMonthDay
                                        xsd:gYear
                                        xsd:gYearMonth
                                        xsd:time
                                      )
                        ] ;
             rdfs:comment "Data property which associates a time interval to its starting time." ;
             rdfs:label "start time"@en .


###  https://w3id.org/polifonia/ontology/core/activityEndDate
core:activityEndDate rdf:type owl:DatatypeProperty ;
                     rdfs:domain core:TimeInterval .


###  https://w3id.org/polifonia/ontology/core/activityStartDate
core:activityStartDate rdf:type owl:DatatypeProperty ;
                       rdfs:domain core:TimeInterval .


###  https://w3id.org/polifonia/ontology/core/endTime
core:endTime rdf:type owl:DatatypeProperty ;
             rdfs:domain core:TimeInterval .


###  https://w3id.org/polifonia/ontology/core/startTime
core:startTime rdf:type owl:DatatypeProperty ;
               rdfs:domain core:TimeInterval .


