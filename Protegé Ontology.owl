#################################################################

###  http://data.doremus.org/ontology#M5_Genre
<http://data.doremus.org/ontology#M5_Genre> rdf:type owl:Class ;
                                            owl:equivalentClass mm:MusicGenre .


###  http://dati.gov.it/onto/dcatapit#Agent
<http://dati.gov.it/onto/dcatapit#Agent> rdf:type owl:Class ;
                                         owl:equivalentClass I0:Agent .


###  http://erlangen-crm.org/efrbroo/F9_Place
<http://erlangen-crm.org/efrbroo/F9_Place> rdf:type owl:Class ;
                                           owl:equivalentClass core:Place .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#Album
mi:Album rdf:type owl:Class ;
         rdfs:comment "An Album is a collection of recordings released by music agents" ;
         rdfs:label "album"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#City
mi:City rdf:type owl:Class ;
        owl:equivalentClass core:City ;
        rdfs:comment "Class to which individual cities that exist in the world belong to." ;
        rdfs:label "city"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#Country
mi:Country rdf:type owl:Class ;
           owl:equivalentClass core:Country ;
           rdfs:comment "Class to which individual cities that exist in the world belong to." ;
           rdfs:label "country"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#Genre
mi:Genre rdf:type owl:Class ;
         owl:equivalentClass mm:MusicGenre ;
         rdfs:comment "Genre is the class of musical genres. Pieces of music, recordings and performances belongs to one or more genres, and several genres can pertain to music agents." ;
         rdfs:label "genre"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#GroupMembershipSituation
mi:GroupMembershipSituation rdf:type owl:Class ;
                            rdfs:subClassOf mi:Situation ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty mi:involvesArtist ;
                                              owl:someValuesFrom mi:MusicArtist
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty mi:involvesGroup ;
                                              owl:someValuesFrom mi:MusicGroup
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty mi:hasTimeInterval ;
                                              owl:allValuesFrom mi:TimeInterval
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty mi:involvesArtist ;
                                              owl:allValuesFrom mi:MusicArtist
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty mi:involvesGroup ;
                                              owl:allValuesFrom mi:MusicGroup
                                            ] ;
                            rdfs:comment "A situation which models the membership of artists to music groups in a time interval" ;
                            rdfs:label "group membership situation"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#LivedInSituation
mi:LivedInSituation rdf:type owl:Class ;
                    rdfs:subClassOf mi:Situation ,
                                    [ rdf:type owl:Class ;
                                      owl:unionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty mi:involvesCity ;
                                                      owl:someValuesFrom mi:City
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty mi:involvesCountry ;
                                                      owl:someValuesFrom mi:Country
                                                    ]
                                                  )
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty mi:involvesArtist ;
                                      owl:someValuesFrom mi:MusicArtist
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty mi:hasTimeInterval ;
                                      owl:allValuesFrom mi:TimeInterval
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty mi:involvesArtist ;
                                      owl:allValuesFrom mi:MusicArtist
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty mi:involvesCity ;
                                      owl:allValuesFrom mi:City
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty mi:involvesCountry ;
                                      owl:allValuesFrom mi:Country
                                    ] ;
                    rdfs:comment "Represents the situation of a music artist living in a certain City and/or Country during a given time interval." ;
                    rdfs:label "lived in situation"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#Lyrics
mi:Lyrics rdf:type owl:Class ;
          rdfs:subClassOf mi:PieceOfMusicRealisation ;
          rdfs:comment "The lyrics of a piece of music" ;
          rdfs:label "lyrics"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#MusicAgent
mi:MusicAgent rdf:type owl:Class ;
              rdfs:subClassOf foaf:Agent ;
              owl:disjointUnionOf ( mi:MusicArtist
                                    mi:MusicGroup
                                  ) ;
              rdfs:comment "A music Agent is an entity which creates pieces of music or realisations of pieces of music such as lyrics, performances, etc. In our case we consider both music artists and music groups as music agents." ;
              rdfs:label "music agent"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#MusicArtist
mi:MusicArtist rdf:type owl:Class ;
               rdfs:subClassOf mi:MusicAgent ,
                               mm:Musician ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty mi:coversMusicRole ;
                                 owl:allValuesFrom mi:MusicRole
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty mi:createsAlbum ;
                                 owl:allValuesFrom mi:Album
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty mi:createsRecording ;
                                 owl:allValuesFrom mi:Recording
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty mi:memberOf ;
                                 owl:allValuesFrom mi:MusicGroup
                               ] ;
               owl:disjointWith mi:MusicGroup ;
               rdfs:comment "A music artist is an individual person which is a music agent." ;
               rdfs:label "music artist"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#MusicGroup
mi:MusicGroup rdf:type owl:Class ;
              rdfs:subClassOf mi:MusicAgent ,
                              mm:MusicEnsenble ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty mi:createsAlbum ;
                                owl:allValuesFrom mi:Album
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty mi:createsRecording ;
                                owl:allValuesFrom mi:Recording
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty mi:hasMember ;
                                owl:allValuesFrom mi:MusicArtist
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty mi:hasMusicRole ;
                                owl:allValuesFrom mi:MusicRole
                              ] ;
              rdfs:comment "A music group is a music agent which is composed of more than one music artist." ;
              rdfs:label "music group"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#MusicRole
mi:MusicRole rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty mi:roleIncluded ;
                               owl:allValuesFrom mi:MusicGroup
                             ] ;
             rdfs:comment "The role a musician has either in a music group or by himself or herself. (e.g.: guitarist, singer, songwriter)" ;
             rdfs:label "music role"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#Performance
mi:Performance rdf:type owl:Class ;
               rdfs:subClassOf mi:PieceOfMusicRealisation ;
               rdfs:comment "A performance which realises a piece of music" ;
               rdfs:label "performance"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#PerformanceSituation
mi:PerformanceSituation rdf:type owl:Class ;
                        rdfs:subClassOf mi:Situation ,
                                        [ rdf:type owl:Class ;
                                          owl:unionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty mi:involvesArtist ;
                                                          owl:someValuesFrom mi:MusicArtist
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty mi:involvesGroup ;
                                                          owl:someValuesFrom mi:MusicGroup
                                                        ]
                                                      )
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty mi:involvesPerformance ;
                                          owl:someValuesFrom mi:Performance
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty mi:involvesArtist ;
                                          owl:allValuesFrom mi:MusicArtist
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty mi:involvesCity ;
                                          owl:allValuesFrom mi:City
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty mi:involvesCountry ;
                                          owl:allValuesFrom mi:Country
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty mi:involvesGroup ;
                                          owl:allValuesFrom mi:MusicGroup
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty mi:involvesPerformance ;
                                          owl:allValuesFrom mi:Performance
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty mi:involvesRole ;
                                          owl:allValuesFrom mi:MusicRole
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty mi:onDate ;
                                          owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                              owl:unionOf ( xsd:date
                                                                            xsd:dateTime
                                                                            xsd:gYear
                                                                            xsd:gYearMonth
                                                                          )
                                                            ]
                                        ] ;
                        rdfs:comment "A situation which models the performance of a piece of music. It possibly involves music agents, cities and/or countries, music roles, performances and dates." ;
                        rdfs:label "performance situation"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#PieceOfMusic
mi:PieceOfMusic rdf:type owl:Class ;
                rdfs:subClassOf core:InformationObject ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty mi:isRealisedBy ;
                                  owl:someValuesFrom mi:PieceOfMusicRealisation
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty mi:isRealisedBy ;
                                  owl:allValuesFrom mi:PieceOfMusicRealisation
                                ] ;
                rdfs:comment "A piece of music is the information object (e.g. a song, a composition) which can be realised, for example, by recordings, scores, lyrics.." ;
                rdfs:label "piece of music"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#PieceOfMusicRealisation
mi:PieceOfMusicRealisation rdf:type owl:Class ;
                           rdfs:subClassOf core:InformationRealization ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty mi:realises ;
                                             owl:someValuesFrom mi:PieceOfMusic
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty mi:realises ;
                                             owl:allValuesFrom mi:PieceOfMusic
                                           ] ;
                           rdfs:comment "A realization of a piece of music" ;
                           rdfs:label "piece of music realisation"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#Recording
mi:Recording rdf:type owl:Class ;
             rdfs:subClassOf mi:PieceOfMusicRealisation ;
             rdfs:comment "A recording which realises a piece of music. It can be included in albums." ;
             rdfs:label "recording"@en .


###  http://www.semanticweb.org/KEProject/MusicalInfluences#ReleaseSituation
mi:ReleaseSituation rdf:type owl:Class ;
                    rdfs:subClassOf mi:Situation ,
                                    [ rdf:type owl:Class ;
                                      owl:unionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty mi:involvesArtist ;
                                                      owl:someValuesFrom mi:MusicArtist
                                                    ]
